@model List<MoneyGuard.Web.Models.clsTransaccion>

@{
    ViewBag.Title = "Transacciones";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Transacciones</h1>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
            </div>
            <div class="panel-body">
                <div class="dataTable_wrapper">
                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                        <thead>
                            <tr>
                                <th>Número cuenta emisor</th>
                                <th>Entidad emisor</th>
                                <th>Número cuenta beneficiario</th>
                                <th>Entidad beneficiario</th>
                                <th>Monto</th>
                                <th>Motivo</th>
                                <th>Tipo</th>
                                <th>Fecha</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (MoneyGuard.Web.Models.clsTransaccion objTransaccion in @Model)
                            {
                                <tr class="odd gradeX">
                                    <td>@objTransaccion.numeroCuentaEmsior</td>
                                    <td>@objTransaccion.entidadEmisor</td>
                                    <td>@objTransaccion.numeroCuentaBeneficiario</td>
                                    <td>@objTransaccion.entidadBeneficiario</td>
                                    <td>$@objTransaccion.montoMovimiento</td>
                                    <td>@objTransaccion.motivoMovimiento</td>
                                    <td>@objTransaccion.Tipo</td>
                                    <td class="center">@objTransaccion.fechaMovimiento</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
